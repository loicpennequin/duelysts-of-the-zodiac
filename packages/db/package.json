{
  "name": "@dotz/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "predev": "yarn start && yarn sync && yarn migrate && yarn seed",
    "dev": "tsup src/index.ts --dts --watch --format esm,cjs",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "sync": "dotenv -c -- prisma generate prisma migrate dev",
    "migrate": "dotenv -c -- prisma migrate dev",
    "migrate:force": "dotenv -c -- prisma db push",
    "ui": "dotenv -c -- prisma studio",
    "seed": "dotenv -c -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seed.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.4.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1"
  }
}
