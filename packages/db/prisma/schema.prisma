generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @unique @default(cuid())
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt
  email     String    @unique
  username  String?
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
