{
  "name": "@dotz/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "dotenv -c -- nodemon",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "db:sync": "dotenv -c -- prisma generate",
    "db:migrate": "dotenv -c -- prisma migrate dev",
    "db:migrate:force": "dotenv -c -- prisma db push",
    "db:ui": "dotenv -c -- prisma studio",
    "db:seed": "dotenv -c -- prisma db seed"
  },
  "dependencies": {
    "@dotz/shared": "*",
    "@dotz/sdk": "*",
    "@prisma/client": "^4.6.1",
    "@trpc/server": "^10.3.0",
    "bcrypt": "^5.1.0",
    "chalk": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "maildev": "^2.0.5",
    "nodemailer": "^6.8.0",
    "prisma": "^4.6.1",
    "socket.io": "^4.5.4",
    "superjson": "^1.11.0",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/maildev": "^0.0.3",
    "@types/node": "^18.11.8",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
